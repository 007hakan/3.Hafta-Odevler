print("*******FIZZ-BUZZ/FIZZBUZZ*******")
try:
    for i in range(1,100):
        if i % 3 == 0 and i % 5 == 0:
            print("FIZZBUZZ")
        if i%3==0:
            print("FIZZ")
        elif i%5==0:
            print("BUZZ")
        else:
            print(i)
except ValueError:
    print("Only numbers allowed!")
